"            ${user.company_id.street}<br/>\n"
"        % endif\n"
"        % if user.company_id.street2:\n"
"            ${user.company_id.street2}<br/>\n"
"        % endif\n"
"        % if user.company_id.city or user.company_id.zip:\n"
"            ${user.company_id.zip} ${user.company_id.city}<br/>\n"
