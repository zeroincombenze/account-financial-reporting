"        % if user.company_id.website:\n"
"            <div>\n"
"                Web :&nbsp;<a href=\"${user.company_id.website}\">${user.company_id.website}</a>\n"
"            </div>\n"
"        %endif\n"
"        <p></p>\n"
"    </div>\n"
"</div>\n"
"            "
